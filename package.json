{
  "name": "gpx-builder",
  "version": "1.0.1",
  "description": "Builder of GPX files",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint --ext .js ./src ./tests",
    "lint-export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -f \"node node_modules/flow-bin/vendor/flow\" -i \"./src/**/*.js*\" -t html -t text -t json -o tests_results/coverage/flow",
    "flow-init": "flow-typed install jest@22.x xmlbuilder@x.x",
    "test": "jest",
    "test-build": "npm run test -- --config jest.config.build.js",
    "build": "rm -rf ./dist && rollup --config ./rollup.config.js",
    "test-all": "npm run lint && npm run flow && npm run test",
    "prepublishOnly": "npm run build && npm run test-build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabulator/gpx-builder.git"
  },
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "license": "Apache License 2.0",
  "keywords": [
    "gpx",
    "garmin",
    "gpx-generator",
    "gps"
  ],
  "homepage": "https://github.com/fabulator/gpx-builder#readme",
  "devDependencies": {
    "@socifi/eslint-config": "^1.9.0",
    "flow-bin": "^0.80.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.3.0",
    "@socifi/jest-config": "^1.9.0",
    "@socifi/rollup-config": "^1.10.0"
  },
  "dependencies": {
    "xmlbuilder": "^10.0.0"
  }
}
